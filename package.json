{
  "name": "@favware/dexa",
  "version": "2.1.0",
  "description": "Amazing pokedex skill for Alexa",
  "author": "@favware",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint src scripts __tests__ --ext mjs,ts --fix",
    "style": "yarn pretty-quick",
    "test": "jest",
    "build": "tsc -b src",
    "watch": "tsc -b src -w",
    "update": "yarn upgrade-interactive --latest",
    "deploy": "node scripts/clean.mjs && node scripts/bundle.mjs && cd bundle && zip -rq function.zip . && aws lambda update-function-code --function-name alexa_dexa --zip-file fileb://function.zip",
    "sversion": "standard-version",
    "scripts:skilldatagenerator": "yarn build && node scripts/skillDataGenerator.mjs",
    "prepare": "husky install .github/husky"
  },
  "dependencies": {
    "@sapphire/utilities": "^3.1.0",
    "alexa-app": "^4.2.3",
    "apollo-boost": "^0.4.9",
    "confusables": "^1.0.0",
    "cross-fetch": "^3.1.4",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@favware/graphql-pokemon": "^6.2.6",
    "@sapphire/eslint-config": "^4.0.8",
    "@sapphire/prettier-config": "^1.2.7",
    "@sapphire/ts-config": "^3.1.6",
    "@types/eslint": "^8",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@types/prettier": "^2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "esbuild": "^0.14.5",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=v16",
    "npm": ">=8"
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "packageManager": "yarn@3.1.1"
}
